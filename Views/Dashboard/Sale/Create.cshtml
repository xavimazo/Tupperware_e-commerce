@{
    Layout = "~/Views/Shared/_LayoutDashboard.cshtml";
}
<div class="container">
    <div class="row">
        <form class="col s12">
            <br />
            <div class="row">
                <div class="input-field col s3">
                    <input id="NombreDelCliente" type="text" class="validate">
                    <label for="last_name">Nombre del cliente</label>
                </div>
                <div class="input-field col s3">
                    <input id="NombreDelProducto" type="text" class="validate">
                    <label for="last_name">Nombre del producto</label>
                </div>
                <div class="input-field col s3">
                    <input id="Cantidad" type="number" class="validate">
                    <label for="last_name">Cantidad</label>
                </div>
                <div class="input-field col s3">
                    <input id="Fecha" type="datetime" class="validate">
                    <label for="last_name">Fecha</label>
                </div>
                <div class="input-field col s3">
                    <a class="waves-effect waves-light btn col s6"><i class="material-icons left">save</i>Guardar producto</a>
                </div>
            </div>
        </form>
    </div>
</div>

