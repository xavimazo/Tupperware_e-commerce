<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="~/Content/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="~/Content/style.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="~/Content/nouislider.min.css" rel="stylesheet" />
</head>

<body>
    <div class="navbar-fixed">
        <nav class="white" role="navigation">
            <div class="nav-wrapper">
                <img src="~/Content/tupperware-logo-black.png" id="Tupperware-logo-black" href="#">
                <ul class="right hide-on-med-and-down">
                    <li><a class="dropdown-button" href="#!" data-activates="dropdownTienda">Productos<i class="material-icons right">arrow_drop_down</i></a></li>
                    <li><a class="dropdown-button" href="#!" data-activates="dropdownExtras">Extras<i class="material-icons right">arrow_drop_down</i></a></li>
                    <li><a href="#">Videos demostracion</a></li>
                    <li><a href="#">Recetas</a></li>
                    <li><a href="#">Caracteristicas especiales</a></li>
                    <li><a href="#">Sobre Nosotros</a></li>
                </ul>
                <a href="#" data-activates="nav-mobile" class="button-collapse"><i class="material-icons">menu</i></a>
            </div>
        </nav>
    </div>
    <ul id="dropdownTienda" class="dropdown-content">
        <li><a href="#!">Basicos de Cocina</a></li>
        <li><a href="#!">Soluciones para beber</a></li>
        <li><a href="#!">Conservacion de alimentos</a></li>
        <li><a href="#!">Transportar</a></li>
        <li><a href="#!">Cocina inteligente</a></li>
        <li><a href="#!">Para compartir</a></li>
        <li><a href="#!">Futuro Kids</a></li>
        <li><a href="#!">Cocina como Chef</a></li>
        <li><a href="#!">Soluciones para el hogar</a></li>
    </ul>
    <ul id="dropdownExtras" class="dropdown-content">
        <li><a href="#!">Bolsos</a></li>
    </ul>
    @RenderBody()
    <footer class="page-footer teal">
        <div class="container">
            <div class="row">
                <div class="col l6 s12">
                    <h5 class="white-text">Sobre nosotros</h5>
                    <p class="grey-text text-lighten-4">Nos encanta Tupperware </p>


                </div>
                <div class="col l3 s12">
                    <h5 class="white-text">Oportunidad</h5>
                    <ul>
                        <li><a class="white-text" href="#!">¡Quiero vender!</a></li>
                        @*<li><a class="white-text" href="#!">Link 2</a></li>
                            <li><a class="white-text" href="#!">Link 3</a></li>
                            <li><a class="white-text" href="#!">Link 4</a></li>*@
                    </ul>
                </div>
                <div class="col l3 s12">
                    <h5 class="white-text">Contacto</h5>
                    <ul>
                        <li><a class="white-text" href="#!"> (+54) 1159197257</a></li>
                        <li><a class="white-text" href="#!">xavi_mazo@live.com.ar</a></li>
                        <li><a class="white-text" href="#!">www.facebook.com/TupperBA</a></li>
                        @*<li><a class="white-text" href="#!">Link 4</a></li>*@
                    </ul>
                </div>
            </div>
        </div>
        <div class="footer-copyright">
            @*<div class="container">*@
            @*Made by <a class="brown-text text-lighten-3" href="http://materializecss.com">Materialize</a>*@
        </div>
    </footer>
    @Scripts.Render("~/bundles/jquery")
    <script src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/materialize.js"></script>
    <script src="~/Scripts/init.js"></script>
    <script src="~/Scripts/nouislider.min.js"></script>
    <script>
        $(document).ready(function () {
            $(".dropdown-button").dropdown({
                constrainWidth: false,
                belowOrigin: true
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
